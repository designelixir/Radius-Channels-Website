<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Questrial&display=swap" rel="stylesheet"> 
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <section class="homepage-hero">
      <header class="site-header">
        <squarespace:navigation navigationId="siteNav" template="site-navigation" />
      </header>
      {@|apply circuitBoard.block}
      <div class="hero-text-container flex-center-start">
        <div>
        {@|apply textRotation.block}
        <div class="flex-center-center flex-wrap" style="margin-top: 50px;">
            <a class="button solid-button" href="/our-mission">Our Mission</a>
            <a class="button solid-button" href="/our-formula">Formula</a>
            <a class="button inverted-button" href="/our-mission">Methodology</a>
        </div>
        
        </div>
      </div>
      <div class="flex-center-center">
            <div class="icon-container box-shadow flex-flexstart-spaceevenly">
                <div class="icon-container-item">
                    <img class="icon-image" src='assets/icons/expertise.png'>
                    <h3>Expertise</h3>
                    <p>Trained by you in your technology, programs, processes, and GTM model.</p>
                </div>

                <div class="icon-container-item">
                    <img src="assets/icons/performance.png" class="icon-image">
                    <h3>Performance Visibility</h3>
                    <p>Digital tools providing you insight on Channel leverage enablement &rarr; Pipeline &rarr; Renewal</p>
                </div>

                <div class="icon-container-item">
                    <img class="icon-image" src='assets/icons/function.png'>
                    <h3>Functional Capabilities</h3>
                    <p>Providing a single point of entry to leverage functional capabilities that include Marketing, Engineering, and Services</p>
                </div>

                <div class="icon-container-item">
                    <img class="icon-image" src='assets/icons/dedicated.png'>
                    <h3>Dedicated Representatives</h3>
                    <p>Off-payroll team, regionally embedded, Channel Specialists with a singular focus on you.</p>
                    
                    
                </div>

            </div>
        
        
        </div>
    </section>
    
    

    <div class="site-container" style="height: 10vh;">

      <!-- cms content injection point -->
      <main class="content-container" role="main" data-content-field="main-content" id="main-section">

        
       {squarespace.main-content}
       
      </main>
    
      <!--Footer with open block field -->
      <footer class="site-footer">
        {@|apply footer-navigation.block}
      </footer>

    </div>

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </body>
</html>


