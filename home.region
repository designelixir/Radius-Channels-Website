<!doctype html>
<html>
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
		{squarespace-headers}
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Questrial&display=swap" rel="stylesheet">
	</head>
	<body id="{squarespace.page-id}" class="{squarespace.page-classes}">
		{@|apply hero.block}
		
			<squarespace:navigation navigationId="siteNav" template="site-navigation" />
		
    <div id="site-wrapper">
<squarespace:block-field id="vendors-description" columns="12"/>
      <section id="hero">
        {@|apply textRotation.block}
        {@|apply downArrow.block}
      </section>
		
		<main id="home" class="content-container" role="main" data-content-field="main-content" id="main-section">
			
			<div class="home-section">
				<squarespace:block-field id="homeSection1" columns="12"/>
			</div>
			<div class="home-section">
				<squarespace:block-field id="homeSection2" columns="12"/>
			</div>
			<div class="home-section">
				<squarespace:block-field id="homeSection3" columns="12"/>
			</div>
			<div class="home-section">
				<squarespace:block-field id="homeSection4" columns="12"/>
			</div>
			<div class="home-section">
				<squarespace:block-field id="homeSection5" columns="12"/>
			</div>
			<div class="home-section">
				<squarespace:block-field id="homeSection6" columns="12"/>
			</div>
			
			{squarespace.main-content}
		</main>
		
		
		
		<!-- cms content injection point -->
		
		<!--Footer with open block field -->
		
			{@|apply footer.block}
		
    </div>
		<!-- combo and minify scripts when not logged in -->
		<squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
		<!-- other scripts, and footer code injection -->
		{squarespace-footers}
	</body>
</html>
