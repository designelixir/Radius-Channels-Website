
{.section item}
<h5 class="breadcrumbs"><a href="/events">&larr; All Events |</a> <span> {title}</span></h5>
<div class="full-size-event box-shadow" id="{title}-fullpage-event">
		<div class="full-size-event-header-wrapper">
			<div class="full-size-event-header box-shadow" {.if assetUrl} style="background-image: url({assetUrl});" {.or} style="background-image: url('../assets/event-bg.png');"{.end}>
		</div>
		<div class="full-size-event-header-title box-shadow">
			{.if customContent.eventType}
			<p>Virtual Event</p>
			{.or}
			<p>In-Person Event</p>
			{.end}
			<h2>{title}</h2>
			<h3 style="color: var(--lightblue); margin: 5px;">{structuredContent.startDate|date %B %d, %Y %I:%M %p} - {structuredContent.endDate|date %B %d, %Y %I:%M %p }</h3>
      <p style="color: gray;">{customContent.eventTimeZone}</p>
      <div class="flex-center-start">
      <p class="strong">Sponsored by: &nbsp;</p>
      {.repeated section tags}
              <p class="tag">{@}</p>{.alternates with}&nbsp;
            {.end}
		</div>
	</div>
	<div class="basic-padding">
		<p>{excerpt}</p>
		<p>{body}</p>
		{.if sourceUrl}
		<button class="solid-button" onclick="window.open('{sourceUrl}')">Register Now</button>
		{.or}
		<button class="solid-button" onclick="window.open('{fullUrl}')">Register Now</button>
		{.end}
	</div>
</div>
{.end}



