
<!-- item loop -->
{.repeated section items}
    
    <div onclick="window.open('{fullUrl}', '_self')" class="supplier-card hover flex-center-start">
        
            <div class="supplier-title">
                {.if assetUrl}
                <img src="{assetUrl}" class="supplier-logo" alt="{title} supplier-logo">
                {.or}
                <h1>{title}</h1>
                {.end}
            </div>
            <div>
                {body}
            </div>
            
        
    </div>
{.end}