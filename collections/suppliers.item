
{.section item}

<div class="supplier-title ">
    <h5 class="breadcrumbs"><a href="/our-suppliers">Our Suppliers |</a> <span> {title}</span></h5>
        {.if assetUrl}
    <img src="{assetUrl}" class="supplier-logo-lg" alt="{title} supplier-logo">
    {.or}
    <h1>{title}</h1>
    {.end}
</div>
{excerpt}
{body}
<div id="upcomingEvents">
<h3>Upcoming Events by {title}</h3>
        <squarespace:query collection="events" limit="5" tag="{title}" category="Upcoming Events">
        

            <div class="calendar-collection-upcoming flex-start-start">
                {.repeated section items}
                <div class="calendar-event hover" onclick="window.open('{fullUrl}')">

                    <div class="calendar-event-date">
                        {customContent.eventStartDate}
                    </div>
                    <h5>{title}</h5>
                </div>
                {.end}
        </squarespace:query>
        <p id="noUpcomingEvents"></p>

<script>  var events = document.getElementsByClassName('calendar-event'); console.log(events); if (events.length === 0) {
    document.getElementById('noUpcomingEvents').innerHTML = 'No upcoming events. Check out all of our live and virtual events <a href="/events">here</a>'

} else {
    document.getElementById('noUpcomingEvents').innerHTML = 'Egg'
}</script>   
    
    
    
    
</div>
{.if items}
<h3>Past Events by {title}</h3>
    <squarespace:query collection="events" limit="3" tag="{title}">

        <div class="calendar-collection-upcoming flex-start-start">
            {.repeated section items}
            <div class="calendar-event hover" onclick="window.open('{fullUrl}')">

                <div class="calendar-event-date">
                    {customContent.eventStartDate}
                </div>
                <h5>{title}</h5>
            </div>
            {.end}



    </squarespace:query>

    {.end}
{.end}

